           <form action="/loan" method="POST" role="form">
 
 <h2>Loan details</h2><br>

    <div class="form-group">
     
      <input type="text" class="form-control" id="bank" name="bank" placeholder="Enter your bank name">
    </div>
    <div class="form-group">
      
      <input type="text" class="form-control" id="branch" name="branch" placeholder="Bank branch name">
    </div>
    <div class="form-group ">
      
      <input type="text" class="form-control" id="ifsc" name="ifsc" placeholder="Enter IFSC code of bank">
    </div>
   
 <div class="form-group ">
      <input type="text" class="form-control" id="account_no" name="account_no" placeholder="Enter your account number">
    </div>
    

    <div class="form-group ">
      <input type="text" class="form-control" id="account_name" name="account_name" placeholder="Enter your name for account">
    </div>

    <div class="form-group">
   
      <input type="text" class="form-control" id="loan_name" name="loan_name" placeholder="Enter the name of your loan">
    </div>

     <div class="form-group">
   
      <input type="text" class="form-control" id="loan_type" name="loan_type" placeholder="Enter type of your loan">
    </div>

    <div class="form-group">
   
      <input type="text" class="form-control" id="loan_id" name="loan_id" placeholder="Enter your loan id">
    </div>
   

    
    <br>
    <input type="hidden" name="_csrf" value="<% _csrf%>" />
    <button type="submit" class="btn btn-default text-center">Submit</button>
    
  </form>
