<!doctype html>
<html lang="en" class="no-js">
<head>
   
    <link rel="stylesheet" href="../../index_files/assets/faq_css/reset.css"> <!-- CSS reset -->
    <link rel="stylesheet" href="../../index_files/assets/faq_css/style.css"> <!-- Resource style -->
    <script src="../../index_files/assets/faq_js/modernizr.js"></script> <!-- Modernizr -->
    <title>Stumuch</title>

</head>
<body>
<%- include ../frontend/header_faq.ejs %>
<section class="cd-faq">
    <ul class="cd-faq-categories" style="padding-top:76px">
        <% _.each(category, function (category, index) { 
            %>
            <li><a  class="<%= (index==0) ? 'selected':'' %> hover_a" href="#<%= category %>"><%= category %></a></li>
            <%
            })
            %>

    </ul> <!-- cd-faq-categories -->

    <div class="cd-faq-items">
         <%
                _.each(category, function (category) {
                %>
        <ul id="<%= category %>" class="cd-faq-group" style="padding-top:55px">
            <li class="cd-faq-title"><h2><%= category %></h2></li>
              <%
                    _.each(faq, function (faq) {
                    if(faq.category==category){
                    %>
            <li>
                <a class="cd-faq-trigger" href="#<%= faq.id %>"><%= faq.name %></a>
                <div class="cd-faq-content">
                    <p> <%= faq.description %></p>
                </div> <!-- cd-faq-content -->
            </li>
       <%
                    }
                    })
                    %>
        </ul> <!-- cd-faq-group -->


                <%
                })
                %>
        <!-- cd-faq-group -->
    </div> <!-- cd-faq-items -->
    <a href="#0" class="cd-close-panel">Close</a>
</section> <!-- cd-faq -->
<script src="../../index_files/assets/faq_js/jquery-2.1.1.js"></script>
<script src="../../index_files/assets/faq_js/jquery.mobile.custom.min.js"></script>
<script src="../../index_files/assets/faq_js/main.js"></script> <!-- Resource jQuery -->
<%- include ../frontend/footer.ejs %>
</body>
</html>