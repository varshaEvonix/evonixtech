                            <input type="hidden" id="loan_percent" value="0">
                            <% } else {%>
                            <p class="text-center indicate"><%= (student_info.total_funded/student_info.loan_amount)*100 %>%</p>
                            <input type="hidden" id="loan_percent" value="<%= (student_info.total_funded/student_info.loan_amount)*100 %>">
                            <% } %>
                        </div>
                        <div class="col-md-4">
                            <p>Goal</p>
                            <h1 style="margin:none !important">$<%= student_info.loan_amount %></h1>
                        </div>
                        <br></div>

                    <br>
                    <div class="text-center don" ><a href="/donorpage/<%= student_info.student_id %>" class="btn btn-success donate_main"><strong style="font-size:22px">Donate Now</strong></a></Button></div>
                    <div class="text-center">
                        <input type="image" src="../../index_files/fbshare.png" alt="Submit" style="width:226px;">

                        <input type="image" src="../../index_files/tshare.png" alt="Submit" style="width:226px;">

                    </div>
                </div>


                <div class="left-panel" id="student_info">
                    <ul class="tab_info">

                        <li class="info_list"><a  class="tablinks active" onclick="openCity(event, 'Story')">My Story</a></li>
                        <li class="info_list"><a  class="tablinks" onclick="openCity(event, 'Ambition')">My Ambitions</a></li>
                        <li class="info_list"><a  class="tablinks" onclick="openCity(event, 'Education')">Education Details</a></li>

                        <li class="info_list"><a class="tablinks" onclick="openCity(event, 'Admin')">Admin notes</a></li>

                    </ul>

                    <div id="Education" class="tabcontent">
                        <p> College/Institute:</p>
                        <p class="Ans"><%= student_info.student_education_institute %></p>


                        <p>Currently Studying:</p>
                        <p class="Ans"><%= student_info.student_education_fieldofstudy %></p>
                    </div>




                    <div id="Admin" class="tabcontent">
                        <p>Admin notes:</p>
                        <% _.each(loan_comments, function (comments) { %>
                        <p class="Ans note_line_height"><%= comments.note %><br/><span class="adminpost">Posted On: <%= comments.last_updated %></span></p>


                        <% }) %>

                    </div>

                    <div id="Story" class="tabstory tabcontent">
                        <p> My Story:</p>
                        <p class="Ans">
                            <%= student_info.student_about_me %></p>

                    </div>



                    <div id="Ambition" class="tabcontent">
                        <p>My Ambitions</p>
                        <p class="Ans"><%= student_info.student_ambition %></p>


                    </div>



                </div>




                <% }) %> 
                <br>
                <div class="panel panel-success" style="">
                    <div class="panel-heading panel-success">Recent donations</div>
                    <div class="panel-body">
                        <ul>
                            <% _.each(donor_l, function (donor_l) { %>
                            <li>
                                <p style="font-size:18px;">$<%= donor_l.funded_amount %><br/><%= donor_l.donors_name %><span class="pull-right adminpost"><%= donor_l.funding_date %></span></p>
                                <%= donor_l.donors_comment %> 

                            </li> <hr>
                            <% }) %>


                        </ul>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
</div><% } else { %>
<div class="row">
    <div class="col-md-12 white_bg"><br><Br>
        <h2 style="text-align:center">PROFILE NOT FOUND</h2><br><Br>
